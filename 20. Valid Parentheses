class Solution {
public:
    bool isValid(string s) {
        stack<char>st;
        for(int i=0;i<s.size();i++)
        {
            if((s[i]==')' || s[i]=='}' || s[i]==']') && st.size()<1 ){
                return(false);
            }
            else if(s[i]=='}'&& st.top()=='{' ||s[i]==')'&& st.top()=='('|| s[i]==']'&& st.top()=='['){
                st.pop();
            }
                else if(s[i]=='(' || s[i]=='{' || s[i]=='[')
                {
                    st.push(s[i]);
                }
            else{
                st.push(s[i]);
            }
        }
        if(st.empty()){
            return(true);
        }
            else
            {
                return(false);
            }
        }
};
